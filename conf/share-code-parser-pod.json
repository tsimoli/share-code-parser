{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "share-code-parser-pod",
    "labels": {
      "app": "share-code-parser-pod"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "share-code-parser",
        "image": "gcr.io/parception-123/share-code-parser:0.1",
        "ports": [
          {
            "containerPort": 9000,
            "protocol": "TCP"
          }
        ]
      }
    ]
  }
}


